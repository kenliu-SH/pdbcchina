{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "start:dev": "UMI_SERVER=dev umi dev",
    "start:test": "UMI_SERVER=test umi dev",
    "start:prod": "UMI_SERVER=prod umi build && http-server dist -p 8000",
    "start:http": "http-server ./dist -p 8000 -d -a 0.0.0.0",
    "build": "umi build",
    "build:dev": "UMI_SERVER=dev umi build",
    "build:test": "UMI_SERVER=test umi build",
    "build:prod": "UMI_SERVER=prod umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@ant-design/pro-layout": "^6.5.0",
    "@rematch/core": "^2.2.0",
    "@rematch/loading": "^2.1.2",
    "@types/lodash": "^4.14.191",
    "antd": "^5.8.5",
    "axios": "^1.3.3",
    "chinese-s2t": "^1.0.0",
    "dayjs": "^1.11.9",
    "file-loader": "^6.2.0",
    "http-server": "^14.1.1",
    "lodash": "^4.17.21",
    "qrcode.react": "^3.1.0",
    "query-string": "^8.1.0",
    "react": "17.x",
    "react-dom": "17.x",
    "react-infinite-scroll-component": "^6.1.0",
    "react-quill": "^2.0.0",
    "react-slick": "^0.29.0",
    "swiper": "^9.1.0",
    "umi": "^3.5.37",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^17.0.0",
    "@types/react-slick": "^0.23.10",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.37",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
