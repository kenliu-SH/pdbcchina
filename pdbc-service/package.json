	"description": "nest.js server development template.",
	"homepage": "",
	"directories": {
		"doc": "docs"
	},
	"scripts": {
		"build": "nest build",
		"lint": "eslint ./src",
		"start": "tsc & cross-env NODE_ENV=prod node dist/main",
		"prod": "cross-env NODE_ENV=prod NODE_PORT=10607 pm2 start dist/main.js --name lz1_server_prod -o /home/logs/out.log -e /home/logs/error.log",
		"prod_master": "cross-env NODE_ENV=prod NODE_PORT=10607 SERVER_ROLE=master pm2 start dist/main.js --name lz1_server_prod -o /home/logs/out.log -e /home/logs/error.log",
		"test": "cross-env NODE_ENV=test NODE_PORT=10608 SERVER_ROLE=master pm2 start dist/main.js --name lz2_server_test",
		"dev": "cross-env NODE_ENV=dev SERVER_ROLE=master nest start --watch",
		"fix=memory-limit": "cross-env LIMIT=84096 increase-memory-limit",
		"pm2:dev": "cross-env NODE_ENV=dev NODE_PORT=10608 SERVER_ROLE=master pm2 start dist/main.js --name lz2_server_dev"
	},
